.actions}>\r\n      <button className={classes[\"button--alt\"]} onClick={props.onClose}>\r\n        Close\r\n      </button>\r\n      {hasItems && (\r\n        <button className={classes.button} onClick={orderHandler}>\r\n          Order\r\n        </button>\r\n      )}\r\n    </div>\r\n  );\r\n\r\n  const cartModalContent =  <React.Fragment>\r\n    {cartItems}\r\n      <div className={classes.total}>\r\n        <span>Total Amount</span>\r\n        <span>{totalAmount}</span>\r\n      </div>\r\n      {isCheckout ? <Checkout onConfirm={submitOrderHandler} onCancel={props.onClose} /> : modalActions}\r\n  </React.Fragment>\r\n\r\n  const isSubmittingModalContent = <p>Sending your Order data....</p>\r\n  const didSubmitModalContent = <React.Fragment>\r\n    <p>Succesfully sent the porder!</p>\r\n    <div className={classes.actions}>\r\n      <button className={classes.button} onClick={props.onClose}>\r\n        Close\r\n      </button> \r\n    </div>\r\n  </React.Fragment> \r\n\r\n  return (\r\n    <Modal onClose={props.onClose}>\r\n      {!isSubmitting && !didSubmit && cartModalContent }\r\n      { isSubmitting && isSubmittingModalContent}\r\n      {!isSubmitting && didSubmit && didSubmitModalContent}\r\n    </Modal>\r\n  );\r\n};\r\n\r\nexport default Cart;\r\n","D:\\Nidhi Vadodariya\\React Practice\\Food Ordering App\\src\\components\\UI\\Modal.js",[],"D:\\Nidhi Vadodariya\\React Practice\\Food Ordering App\\src\\store\\CartProvider.js",[],"D:\\Nidhi Vadodariya\\React Practice\\Food Ordering App\\src\\store\\cart-context.js",[],"D:\\Nidhi Vadodariya\\React Practice\\Food Ordering App\\src\\components\\Cart\\CartItem.js",[],"D:\\Nidhi Vadodariya\\React Practice\\Food Ordering App\\src\\components\\Cart\\Checkout.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":1,"column":8,"nodeType":"95","messageId":"96","endLine":1,"endColumn":13},{"ruleId":"93","severity":1,"message":"97","line":1,"column":